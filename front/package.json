{
  "name": "react-ts-redux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/dayjs": "^2.14.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "5.8.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-redux": "^7.1.24",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "oidc-client-ts": "^2.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "cross-env": "^7.0.3",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "^8.0.0",
    "mkcert": "^1.5.0",
    "npm-check-updates": "^13.0.1",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.0"
  },
  "scripts": {
    "start": "craco start",
    "starts": "cross-env HTTPS=true npm run start",
    "starts2": "cross-env HTTPS=true SSL_CRT_FILE=cert.crt SSL_KEY_FILE=cert.key npm run start",
    "test": "craco test --watchAll=false",
    "test:watch": "craco test --watch",
    "build": "craco build",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext tsx,ts",
    "lint:fix": "eslint ./src --ext tsx,ts --fix",
    "prettier": "prettier-eslint 'src/**/*.{ts,tsx}' ",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@components/(.*)$": "<rootDir>/src/components/$1",
      "^@router/(.*)$": "<rootDir>/src/router/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1",
      "^@enums/(.*)$": "<rootDir>/src/enums/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@store/(.*)$": "<rootDir>/src/store/$1"
    }
  }
}
